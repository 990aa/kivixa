{
    "files.exclude": {
        "android/.gradle/": true,
        "android/.kotlin/": true,
        "submodules/*/*": true,
        "linux/flutter/ephemeral/": true,
        "**/.dart_tool/": true,
        "**/.idea/": true,
        "**/.flutter-plugins-dependencies": true,
    },
    "files.associations": {
        "VERSION": "plaintext"
    },
    "problems.excludeGitIgnore": true,
    "search.exclude": {
        "**/VERSION": true,
        "**/build.gradle.kts": true
    },
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true,
    "dart.doNotFormat": [
        "**/submodules/**",
    ],
    "dart.analysisExcludedFolders": [
        "submodules",
        "packages"
    ],
    "cmake.ignoreCMakeListsMissing": true,
    "groovyLint.exclude": [
        "**/rust_builder/cargokit/**"
    ],
    "java.project.resourceFilters": [
        "rust_builder/cargokit"
    ]
}
